SELECT 
    NO_REGISTRATION,
    PASIEN_ID,
    NAME_OF_PASIEN,
    CONVERT(date, DATE_OF_BIRTH) AS DATE_OF_BIRTH,
    CASE 
        WHEN GENDER = 1 THEN 'Laki-laki'
        WHEN GENDER = 2 THEN 'Perempuan'
        ELSE NULL
    END AS GENDER,
    CONTACT_ADDRESS,
    PHONE_NUMBER
FROM SIMRS.dbo.PASIEN
ORDER BY NO_REGISTRATION;
